{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "allowJs": true,
    "experimentalDecorators": true,
    "useDefineForClassFields": false,
  },
  "exclude": [
    "node_modules",
  ]
}
